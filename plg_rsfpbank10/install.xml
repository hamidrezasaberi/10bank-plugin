<?xml version="1.0" encoding="utf-8"?>
<install type="component" version="1.5" method="upgrade">
<name>rsinstaller</name>
<author>bank10</author>
<creationDate>August 2015</creationDate>
<copyright>(C) 2015 www.10bank.ir</copyright>
<authorEmail>admin@10bank.ir</authorEmail>
<authorUrl>www.10bank.ir</authorUrl>
<version>1.3.0</version>
<description><![CDATA[RSForm!Pro - bank10 plugin allows you to add the bank10 payment procedure to your form. Works with at least RSForm! Pro 1.3.0 REV30!]]></description>
	<install>
		<sql>
			<file driver="mysql">install.mysql.rsfpbank10.sql</file>
			<file driver="mysql" charset="utf8">install.mysql.rsfpbank10.sql</file>
		</sql>
	</install>
	<uninstall>
		<sql>
			<file driver="mysql">uninstall.mysql.rsfpbank10.sql</file>
			<file driver="mysql" charset="utf8">uninstall.mysql.rsfpbank10.sql</file>
		</sql>
	</uninstall>
	<scriptfile>script.rsinstaller.php</scriptfile>
	<installfile>install.rsinstaller.php</installfile>
	<administration>
		<files>
			<filename>install.mysql.rsfpbank10.sql</filename>
			<filename>uninstall.mysql.rsfpbank10.sql</filename>
		</files>
	</administration>
</install>